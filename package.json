    "concurrently": "^4.1.0",
    "connect-mongo": "^3.1.2",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-session": "^1.17.0",
    "mocha": "^6.2.1",
    "mongoose": "^5.7.1",
